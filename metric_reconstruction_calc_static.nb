             RowBox[{"fout", "=", 
              RowBox[{"N", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{
                  RowBox[{
                   RowBox[{
                    RowBox[{
                    RowBox[{
                    RowBox[{
                    RowBox[{"Vat", "[", 
                    RowBox[{"[", "qi", "]"}], "]"}], "[", 
