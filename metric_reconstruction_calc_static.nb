Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "Check", " ", "continuity", " ", "and", " ", "jumps", " ", "in", " ", 
    "first", " ", "derivatives"}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"v0jump", "=", 
     RowBox[{"GetEmptyVat", "[", "lmax", "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"v1jump", "=", 
     RowBox[{"GetEmptyVat", "[", "lmax", "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Monitor", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"For", "[", 
        RowBox[{
         RowBox[{"qi", "=", "1"}], ",", 
         RowBox[{"qi", "<=", "5"}], ",", 
         RowBox[{"qi", "++"}], ",", "\[IndentingNewLine]", 
